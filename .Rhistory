GetSynonymesFromWordNet <- function(synonymes)
{
setDict("C:/Program Files (x86)/WordNet/2.1/dict")
result <- NULL
for(i in synonymes)
{
{
result <- synonyms(i, "NOUN")
print(paste("print",result,sep = " "))
result <- data.table(rbind(result,data.table("terms" = result, "base" = i)))
}
}
return(result)
}
